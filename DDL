CREATE TABLE demo.news_catalog.articles (
                    key STRING,
                    id STRING,
                    title STRING,
                    description STRING,
                    author STRING,
                    url STRING,
                    published TIMESTAMP,
                    category STRING,
                    language STRING,
                    tags STRING,
                    processing_timestamp TIMESTAMP
                ) USING iceberg
                PARTITIONED BY (days(published), category)
                TBLPROPERTIES (
                    'format-version'='2');